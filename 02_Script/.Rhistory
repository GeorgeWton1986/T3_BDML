transmi_sin_chapinero <- transmi_sf_bogota %>%
filter(
lon < limites_chapinero[1, "min"] | lon > limites_chapinero[1, "max"] |
lat < limites_chapinero[2, "min"] | lat > limites_chapinero[2, "max"]
)
# Visualizar las estaciones fuera de Chapinero
leaflet() %>%
addTiles() %>%
addCircles(data = transmi_sin_chapinero,
color = "blue",
radius = 50,
label = ~as.character(name))
View(train_localidades)
# Obtener bbox de Bogotá
limites_bogota <- getbb("Bogotá, Colombia")
# Obtener CAI (policías) en Bogotá
cai_query_bogota <- opq(bbox = limites_bogota) %>%
add_osm_feature(key = "amenity", value = "police")
# Obtener datos de CAI
cai_data_bogota <- osmdata_sf(cai_query_bogota)
cai_sf_bogota <- cai_data_bogota$osm_points %>% dplyr::select(osm_id, name)
cai_sf_bogota <- st_transform(cai_sf_bogota, 4326)
# Convertir apartamentos fuera de Chapinero a sf
aptos_sf_sin_chapinero <- st_as_sf(train_localidades, coords = c("lon", "lat"), crs = 4326)
# Calcular distancias entre los apartamentos y los CAI de Bogotá
dist_matrix_cai_sin_chapinero <- st_distance(aptos_sf_sin_chapinero, cai_sf_bogota)
dist_min_cai_sin_chapinero <- apply(dist_matrix_cai_sin_chapinero, 1, min)
# Agregar la distancia mínima a la base de datos
train_localidades <- train_localidades %>%
mutate(distancia_cai = dist_min_cai_sin_chapinero)
# Visualizar las distancias a los CAI
ggplot(train_localidades, aes(x = distancia_cai)) +
geom_histogram(bins = 50, fill = "darkred", alpha = 0.5) +
labs(x = "Distancia mínima a un CAI (m)", y = "Cantidad",
title = "Distribución de la distancia a los CAI en el resto de Bogotá") +
theme_bw()
# Guardar el gráfico
ggsave("../04_Views/GRAFICO8C-DISTANCIACAI_RESTO.png", dpi = 150, width = 8, height = 6)
# Definir el área de Bogotá (cubre todas las localidades)
