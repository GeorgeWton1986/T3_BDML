# Descargar vías principales en Bogotá
vias_query_bogota <- opq(bbox = limites_bogota) %>%
add_osm_feature(key = "highway", value = c("primary", "trunk"))
# Obtener datos como sf
vias_data_bogota <- osmdata_sf(vias_query_bogota)
# Usar líneas de vías principales si están disponibles
vias_sf_bogota <- vias_data_bogota$osm_lines %>% dplyr::select(osm_id, name)
vias_sf_bogota <- st_transform(vias_sf_bogota, 4326)
# Convertir apartamentos a sf si no lo están
aptos_sf_bogota <- st_as_sf(train_localidades, coords = c("lon", "lat"), crs = 4326)
# Calcular distancias a las avenidas principales más cercanas
dist_matrix_vias_bogota <- st_distance(aptos_sf_bogota, vias_sf_bogota)
